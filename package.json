{
  "name": "stripe-embeded",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "dev": "yarn clean && yarn webpack && yarn copy-assets && yarn tsc && vercel dev",
    "build": "yarn clean && yarn webpack && yarn compile && yarn copy-assets",
    "clean": "rimraf dist",
    "webpack": "npx webpack",
    "compile": "tsc",
    "copy-assets": "mkdirp dist && cp public/style.css dist/style.css && cp public/index.html dist/index.html",
    "prebuild": "yarn clean",
    "postbuild": "yarn copy-assets",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "babel-loader": "^9.2.1",
    "husky": "^9.1.7",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@stripe/connect-js": "^3.3.19",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "stripe": "^16.5.0"
  }
}